{
    "version": 2,
    "buildCommand": "bash build_files.sh",
    "outputDirectory": "staticfiles_build",
    "rewrites": [
        {
            "source": "/static/(.*)",
            "destination": "/static/$1"
        },
        {
            "source": "/media/(.*)",
            "destination": "/media/$1"
        },
        {
            "source": "/(.*)",
            "destination": "school_management/wsgi.py"
        }
    ]
}