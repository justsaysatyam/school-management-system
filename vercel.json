{
    "version": 2,
    "buildCommand": "./build_files.sh",
    "outputDirectory": "staticfiles_build",
    "functions": {
        "school_management/wsgi.py": {
            "maxLambdaSize": "15mb",
            "runtime": "python3.12"
        }
    },
    "rewrites": [
        {
            "source": "/static/(.*)",
            "destination": "/static/$1"
        },
        {
            "source": "/media/(.*)",
            "destination": "/media/$1"
        },
        {
            "source": "/(.*)",
            "destination": "school_management/wsgi.py"
        }
    ]
}